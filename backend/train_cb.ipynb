   "source": [
    "input_path =  os.path.join(os.path.join(os. getcwd(),'mlrec\\\\models_data'),'12_content-based_dataset.csv')\n",
    "input_path\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "a6323d69-fbf9-4dbe-b085-a36ec70bf79e",
   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.read_csv(input_path)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "b1000b25-138c-4b23-81e7-8ea13dd22ad4",
   "metadata": {},
   "outputs": [],
   "source": [
    "df['description'] = df['description'].fillna('')\n",
    "df['description'] = df.apply(lambda row: row['title'] + ': ' + row['description'], axis=1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "07598bac-18bf-4002-a3e7-c3d525dcd8eb",
   "metadata": {},
   "outputs": [],
   "source": [
    "tfidf = TfidfVectorizer(stop_words='english')\n",
    "tfidf_matrix = tfidf.fit_transform(df['description'])\n",
    "cosine_sim = linear_kernel(tfidf_matrix, tfidf_matrix)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "2acccea3-3303-4e85-a9b0-6132004e95e3",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_recommendations(movie_index, cosine_sim=cosine_sim, num_recommend = 10):\n",
    "    idx = movie_index # Get the pairwsie similarity scores of all movies with that movie\n",
    "    sim_scores = list(enumerate(cosine_sim[idx])) # Sort the movies based on the similarity scores\n",
    "    sim_scores = sorted(sim_scores, key=lambda x: x[1], reverse=True) # Get the scores of the 10 most similar movies\n",
    "    top_similar = sim_scores[0:num_recommend+1] # Get the movie indices\n",
    "    movie_indices = [i[0] for i in top_similar] # Return the top 10 most similar movies\n",
    "    return df.iloc[movie_indices]\n"
   ]
  },
